<h1>Student Form</h1>
<%= form_tag students_path do%>
    <!--Get first and last name of student and send it to the Students#create-->
    <label>First name:</label>
    <%= text_field_tag :"student[first_name]"%><br>

    <label>Last name:</label>
    <%= text_field_tag :"student[last_name]"%><br>

    <!--<input type="submit" name="Submit Student">-->
    <%= submit_tag "Submit Student"%>

<%end%>